package new01;

public class new01 {

	public static void main(String[] args) {
		/* 홍길동 주민등록번호 881122-1234567이다
		 * 출력형태 => 생년월일 : 881122, 성별 : 남(1,3)
		 * 콘솔에 출력
		 */
		
		String pin = "881122-1234567";
		String birth = pin.substring(0,6);
		char wm = pin.charAt(7);
		System.out.println(wm);
		String a = "";
		if(wm == "1" || wm == "3") {
			a="남";
		}else {
			a="여";
		}
				
		System.out.println("생년월일 :, "+birth+" 성별 :"+a );
		
	}

}
